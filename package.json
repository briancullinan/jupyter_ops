{
    "name": "Jupytangular",
    "description": "Automation Notebooks in Jupyter",
    "license": "UNLICENSED",
    "scripts": {
        "start": "",
        "export": "node -e \"require('./Core').import('export deploy notebook')(['firebase rpc wrapper', 'tell joke', 'latest s3 bucket', 'calendar search heatmap', 'spawn child process'])\"",
        "prebuild": "npm run export",
        "build": "webpack",
        "selenium": "selenium-standalone start",
        "test": "node -e \"require('./Core').import('test cells')('test test runner')\" && npm run pull",
        "lint": "eslint . --fix",
        "deploy": "git --git-dir=../jupytangular/.git reset --hard && git --git-dir=../jupytangular/.git pull --rebase",
        "pull": "node -e \"require('./Core').import('update git')()\""
    },
    "engines": {
        "node": ">= 8",
        "npm": ">= 4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/megamindbrian/jupytangular.git"
    },
    "dependencies": {
        "@angular-devkit/schematics": "0.0.43",
        "@angular/cli": "^1.6.0",
        "@google/maps": "^0.4.5",
        "aws-sdk": "^2.175.0",
        "bluebird": "^3.5.1",
        "body-parser": "^1.18.2",
        "chai": "^4.1.2",
        "chrono-node": "^1.3.5",
        "cors": "^2.8.4",
        "d3-cloud": "^1.2.4",
        "d3-node": "^1.1.2",
        "daemon": "^1.1.0",
        "diff": "^3.4.0",
        "fibers": "^2.0.0",
        "firebase-admin": "^5.8.0",
        "firebase-functions": "^0.7.3",
        "fuse.js": "^3.2.0",
        "glob": "^7.1.2",
        "google-auth-library": "^0.10.0",
        "googleapis": "^20.2.0",
        "imap-simple": "^4.0.0",
        "jsdom": "^11.5.1",
        "jsonpath-plus": "^0.16.0",
        "lockfile": "^1.0.3",
        "memory-fs": "git+https://github.com/webpack/memory-fs.git",
        "min-document": "^2.19.0",
